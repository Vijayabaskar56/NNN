{
  "name": "final-setup",
  "license": "0BSD",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "cross-env EXPO_NO_DOTENV=1 expo start",
    "prebuild": "cross-env EXPO_NO_DOTENV=1 bun expo prebuild",
    "android": "cross-env EXPO_NO_DOTENV=1 expo run:android",
    "ios": "cross-env EXPO_NO_DOTENV=1 expo run:ios",
    "web": "cross-env EXPO_NO_DOTENV=1 expo start --web",
    "xcode": "xed -b ios",
    "doctor": "bunx expo-doctor@latest",
    "preinstall": "bunx only-allow bun",
    "start:staging": "cross-env APP_ENV=staging bun run start",
    "prebuild:staging": "cross-env APP_ENV=staging bun run prebuild",
    "prebuild:development": "cross-env APP_ENV=development bun run prebuild",
    "android:staging": "cross-env APP_ENV=staging bun run android",
    "ios:staging": "cross-env APP_ENV=staging bun run ios",
    "start:production": "cross-env APP_ENV=production bun run start",
    "prebuild:production": "cross-env APP_ENV=production bun run prebuild",
    "android:production": "cross-env APP_ENV=production bun run android",
    "ios:production": "cross-env APP_ENV=production bun run ios",
    "build:development:ios": "cross-env APP_ENV=development EXPO_NO_DOTENV=1 eas build --profile development --platform ios",
    "build:development:android": "cross-env APP_ENV=development EXPO_NO_DOTENV=1 eas build --profile development --platform android ",
    "build:staging:ios": "cross-env APP_ENV=staging EXPO_NO_DOTENV=1 eas build --profile staging --platform ios",
    "build:staging:android": "cross-env APP_ENV=staging EXPO_NO_DOTENV=1 eas build --profile staging --platform android ",
    "build:production:ios": "cross-env APP_ENV=production EXPO_NO_DOTENV=1 eas build --profile production --platform ios",
    "build:production:android": "cross-env APP_ENV=production EXPO_NO_DOTENV=1 eas build --profile production --platform android ",
    "prepare": "husky",
    "app-release": "cross-env SKIP_BRANCH_PROTECTION=true np --no-publish --no-cleanup --no-release-draft",
    "version": "bun run prebuild && git add .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc  --noemit",
    "lint:translations": "eslint ./src/translations/ --fix --ext .json  ",
    "test": "jest",
    "check-all": "bun run lint && bun run type-check && bun run lint:translations && bun run test",
    "test:ci": "bun run test --coverage",
    "test:watch": "bun run test --watch",
    "install-maestro": "curl -Ls 'https://get.maestro.mobile.dev' | bash",
    "e2e-test": "maestro test .maestro/ -e APP_ID=com.obytes.development"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@better-auth/expo": "^0.7.1",
    "@expo/html-elements": "0.4.2",
    "@expo/vector-icons": "^14.0.2",
    "@gluestack-ui/accordion": "^1.0.7",
    "@gluestack-ui/nativewind-utils": "^1.0.23",
    "@gluestack-ui/overlay": "^0.1.15",
    "@gluestack-ui/toast": "^1.0.7",
    "@gluestack-ui/tooltip": "^0.1.34",
    "@legendapp/motion": "^2.4.0",
    "@react-navigation/bottom-tabs": "7.0.0-rc.36",
    "@react-navigation/native": "7.0.0-rc.21",
    "app-icon-badge": "^0.1.1",
    "better-auth": "^0.7.1",
    "drizzle-orm": "^0.36.0",
    "expo": "~52.0.0-preview.16",
    "expo-blur": "~14.0.1",
    "expo-constants": "~17.0.2",
    "expo-font": "~13.0.0",
    "expo-haptics": "~14.0.0",
    "expo-linking": "~7.0.2",
    "expo-localization": "~16.0.0",
    "expo-router": "~4.0.0-preview.11",
    "expo-splash-screen": "~0.29.1",
    "expo-sqlite": "~14.0.6",
    "expo-status-bar": "~2.0.0",
    "expo-symbols": "~0.2.0",
    "expo-system-ui": "~4.0.2",
    "expo-web-browser": "~14.0.0",
    "lucide-react-native": "^0.454.0",
    "nativewind": "^4.1.21",
    "pg": "^8.13.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.1",
    "react-native-edge-to-edge": "^1.0.1",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "4.0.0-beta.16",
    "react-native-svg": "^15.8.0",
    "react-native-web": "~0.19.13",
    "tailwindcss": "^3.4.14",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@dev-plugins/react-query": "^0.0.7",
    "@expo/config": "^10.0.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react-native": "^12.7.2",
    "@types/i18n-js": "^3.8.9",
    "@types/jest": "^29.5.12",
    "@types/lodash.memoize": "^4.1.9",
    "@types/pg": "^8.11.10",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.27.1",
    "eslint": "^8.57.1",
    "eslint-config-expo": "~8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-i18n-json": "^4.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.15.2",
    "eslint-plugin-testing-library": "^6.2.2",
    "eslint-plugin-unicorn": "^46.0.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^9.1.5",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.7.0",
    "jest-expo": "~52.0.0-preview.3",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.2.9",
    "np": "^10.0.7",
    "prettier": "^3.3.3",
    "react-test-renderer": "18.3.1",
    "ts-jest": "^29.1.2",
    "tsx": "^4.19.2",
    "typescript": "^5.3.3"
  },
  "private": true
}
